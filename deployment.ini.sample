[DEFAULT]
debug = false

[app:django]
use = egg:SMS-CVS
settings = %(here)s/settings.py

[app:docs]
use = egg:Paste#static
document_root = %(here)s/docs/html

[composite:main]
use = egg:Paste#urlmap
/media = media
/docs = docs
/ = django

[server:main]
use = egg:Flup#scgi_thread
host = 127.0.0.1
port = 8080